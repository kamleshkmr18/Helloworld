package com.kamlesh;

import java.util.Arrays;
import java.util.Comparator;
import java.util.List;

public class Test {

	public static void main(String[] args) {
		int[] intArr = {2,5,7,2,10,25,7,12,10,25};
		List lst = Arrays.asList(intArr);
		lst.stream().distinct().sorted(Comparator.reverseOrder()).limit(3).forEach(System.out::println);
	}

}
